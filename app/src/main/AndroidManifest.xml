<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mohit.flipcoin">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_coin_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_coin_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.FlipCoin">
        <activity android:name=".TossCoin"></activity>
        <activity android:name=".Selection" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>